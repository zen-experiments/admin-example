{
    "presets": [
        ["env", {
            "targets": {
                "node": "current",
                "browsers": [
                    "last 1 Chrome versions"
                ]
            },
            "loose": true,
            "modules": false,
            "debug": true
        }],
        "react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "transform-object-rest-spread",
        "syntax-dynamic-import"
    ]
}
